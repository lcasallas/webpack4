{
	"name": "webpack4",
	"version": "1.0.0",
	"description": "curso webpack version 4",
	"main": "index.js",
	"scripts": {
		"build": "webpack --entry ./index --output ./bundle.js --mode development",
		"build:local": "webpack --mode production",
		"build:external": "webpack --config ./external/webpack.config.js",
		"build:multi": "webpack --config ./multi-entry-points/webpack.config.js",
		"build:cssloader": "webpack --config ./css-style-loader/webpack.config.js",
		"build:plugins": "webpack --config ./use-plugins/webpack.config.js",
		"build:dev": "webpack-dev-server --config ./dev-server/webpack.config.js",
		"build:babel": "webpack-dev-server --config ./use-babel/webpack.config.js",
		"build:react": "webpack-dev-server --config ./use-react/webpack.config.js",
		"build:url-loader": "webpack-dev-server --config ./use-url-loader/webpack.config.js",
		"build:prevent-duplication": "webpack --config ./use-prevent-duplication/webpack.config.js",
		"build:dll": "webpack --config ./use-dynamic-library/webpack.dll.config.js",
		"build:prevent-duplication-fast": "webpack --config ./use-dynamic-library/webpack.config.js",
		"build:dynamic-dll": "webpack --config ./use-dynamic-import/webpack.dll.config.js",
		"build:dynamic-load": "webpack --config ./use-dynamic-import/webpack.config.js"
	},
	"author": "@LeCode",
	"license": "MIT",
	"dependencies": {
		"@babel/runtime": "^7.8.4",
		"react": "^16.12.0",
		"react-dom": "^16.12.0"
	},
	"devDependencies": {
		"@babel/core": "^7.8.4",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/plugin-transform-runtime": "^7.8.3",
		"@babel/preset-env": "^7.8.4",
		"@babel/preset-react": "^7.8.3",
		"babel-loader": "^8.0.6",
		"css-loader": "^3.4.2",
		"file-loader": "^5.1.0",
		"html-webpack-plugin": "^3.2.0",
		"less": "^3.11.1",
		"less-loader": "^5.0.0",
		"mini-css-extract-plugin": "^0.9.0",
		"node-sass": "^4.13.1",
		"postcss-loader": "^3.0.0",
		"sass-loader": "^8.0.2",
		"style-loader": "^1.1.3",
		"stylus": "^0.54.7",
		"stylus-loader": "^3.0.2",
		"url-loader": "^3.0.0",
		"webpack": "^4.41.6",
		"webpack-cli": "^3.3.11",
		"webpack-dev-server": "^3.10.3"
	}
}
