{
	"name": "webpack4",
	"version": "1.0.0",
	"description": "curso webpack version 4",
	"main": "index.js",
	"scripts": {
		"build": "webpack --entry ./index --output ./bundle.js --mode development",
		"build:local": "webpack --mode production",
		"build:external": "webpack --config ./external/webpack.config.js",
		"build:multi": "webpack --config ./multi-entry-points/webpack.config.js",
		"build:cssloader": "webpack --config ./css-style-loader/webpack.config.js",
		"build:plugins": "webpack --config ./use-plugins/webpack.config.js",
		"build:dev": "webpack-dev-server --config ./dev-server/webpack.config.js",
		"build:babel": "webpack-dev-server --config ./use-babel/webpack.config.js"
	},
	"author": "@LeCode",
	"license": "MIT",
	"dependencies": {},
	"devDependencies": {
		"@babel/core": "^7.8.4",
		"@babel/preset-env": "^7.8.4",
		"babel-loader": "^8.0.6",
		"css-loader": "^3.4.2",
		"html-webpack-plugin": "^3.2.0",
		"mini-css-extract-plugin": "^0.9.0",
		"style-loader": "^1.1.3",
		"webpack": "^4.41.6",
		"webpack-cli": "^3.3.11",
		"webpack-dev-server": "^3.10.3"
	}
}
